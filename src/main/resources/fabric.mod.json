{
	"schemaVersion": 1,
	"id": "sapphire-mod",
	"version": "${version}",
	"name": "Sapphire Mod",
	"description": "A simple mod adding sapphires to minecraft.",
	"authors": [
		"Samuel Kuhn"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/sapphire-mod/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"sapphiremod.SapphireMod"
		],
		"client": [
			"sapphiremod.SapphireModClient"
		],
		"fabric-datagen": [
			"sapphiremod.SapphireModDataGenerator"
		]
	},
	"mixins": [
		"sapphire-mod.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.10",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}